<div class="container">
  <h1>Add a review <%= @restaurant.name %></h1>
  <%= simple_form_for([@restaurant, @review]) do |f|%>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= link_to "Back to list", restaurants_path, class: "btn btn-primary"%>
   <%= f.submit "Your review", class: "btn btn-primary" %>
  <% end %>
  </div>
