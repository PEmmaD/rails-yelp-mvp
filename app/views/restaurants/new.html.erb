<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name  %>
  <%=  f.input :address  %>
  <%= f.input :phone_number  %>
  <%= f.input :category   %>
  <%= f.submit "add a restaurant", class: "btn btn-primary"
  %>
  <%= link_to "Back to the list", restaurants_path, class: "btn btn-primary"%>
<% end %>
